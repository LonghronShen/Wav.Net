        "System.Globalization": "4.0.0.0",
        "System.IO": "4.0.0.0",
        "System.Linq": "4.0.0.0",
        "System.Linq.Expressions": "4.0.0.0",
        "System.Linq.Queryable": "4.0.0.0",
        "System.Reflection": "4.0.0.0",
        "System.Reflection.Extensions": "4.0.0.0",
        "System.Reflection.Primitives": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Runtime.Extensions": "4.0.0.0",
        "System.Runtime.InteropServices": "4.0.0.0",
        "System.Text.RegularExpressions": "4.0.0.0"
      },
      "buildOptions": {
        "define": [ "PORTABLE" ]
